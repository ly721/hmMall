/* pages/indexs/indexs.wxss */
view{
    color: var(--themeColor);
}
swiper {
   width: 750rpx;
   height: 340rpx;
}
swiper swiper-item navigator  image{
    width: 100%;
}


.navi {
    display: flex;
}
.navi navigator {
  padding: 20rpx;
  flex: 1;
}
.navi navigator image {
    width:100%;
}


.floor_title {
    width: 750rpx;
    height: 59rpx;
    margin: 10rpx 0;
}
.floor_item {
    /* 清除浮动 */
    overflow: hidden;
}
.floor_item navigator{
   float: left;
   width: 33.3%;
}
/* 后几个超链接 */
.floor_item navigator:nth-last-child(-n+4) image {
    height: calc(33.3vw * 386/232/2);
    border-left: 10rpx solid #fff;
 }
 /* .floor_item navigator:nth-child(2),.floor_item navigator:nth-child(3){
     border-bottom: 10rpx solid #fff;
 } */
.floor_item navigator image {
    width: 100%;
    height: 100%;
 }
